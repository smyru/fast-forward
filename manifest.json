{
    "manifest_version": 2,
    "name": "Fast Forward",
    "description": "This extension fast rewinds you ;-) to bring back part of old Opera UX goodies allowing for predictive navigation within websites",
    "version": "0.4",
    "background": {
        "page": "background.html"
    },
    "permissions": [ "tabs" ],
    "content_scripts": [
        {
            "matches": [
                "http://*/*",
                "https://*/*"
            ],
            "js": [
                "scripts/content.js"
            ]
        }
    ],
    "icons": {
        "128": "icons/icon-128.png",
        "96": "icons/icon-96.png",
        "64": "icons/icon-64.png"
    }
}
